type EthRpcResult = variant {
  Ok: blob;
  Err : opt variant {
    NoPermission;
    TooFewCycles : text;
    ServiceUrlParseError;
    ServiceUrlHostMissing;
    ServiceUrlHostNotAllowed;
    HttpRequestError : record { code: nat32; message: text };
  }
};
service : {
  eth_rpc_request: (json_rpc_payload: text, service_url: text, max_response_bytes: nat64) -> (EthRpcResult);
}
